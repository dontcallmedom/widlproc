<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Definitions SYSTEM "widlprocxml.dtd">
<Definitions>
  <Exception name="InvalidName" id="::InvalidName">
    <webidl>exception InvalidName {
  DOMString reason;
};</webidl>
    <ExceptionField name="reason" id="::InvalidName::reason">
      <webidl>  DOMString reason;</webidl>
      <Type type="DOMString"/>
    </ExceptionField>
  </Exception>
  <Exception name="NoSuchPet" id="::NoSuchPet">
    <webidl>exception NoSuchPet { };</webidl>
  </Exception>
  <Interface name="Person" id="::Person">
    <webidl>interface Person {

  attribute unsigned short age;

  attribute DOMString name setraises (<ref>InvalidName</ref>);

  readonly attribute DOMString petName getraises (<ref>NoSuchPet</ref>);
};</webidl>
    <Attribute name="age" id="::Person::age">
      <webidl>  attribute unsigned short age;</webidl>
      <Type type="unsigned short"/>
    </Attribute>
    <Attribute name="name" id="::Person::name">
      <webidl>  attribute DOMString name setraises (<ref>InvalidName</ref>);</webidl>
      <Type type="DOMString"/>
      <SetRaises>
        <RaiseException name="InvalidName"/>
      </SetRaises>
    </Attribute>
    <Attribute readonly="readonly" name="petName" id="::Person::petName">
      <webidl>  readonly attribute DOMString petName getraises (<ref>NoSuchPet</ref>);</webidl>
      <Type type="DOMString"/>
      <GetRaises>
        <RaiseException name="NoSuchPet"/>
      </GetRaises>
    </Attribute>
  </Interface>
</Definitions>
